<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
	<class name="Agente" table="Agente">
		<id name="CodAgente" column="CodAgente">
			<generator class="identity" />
		</id>
		<property name="RazonSocial" column="RazonSocial" />
		<property name="RUC" column="RUC" />
		<property name="CorreoAgente" column="CorreoAgente" />
		<property name="Direccion" column="Direccion" />
		<property name="NroCuentaInterbancaria" column="NroCuentaInterbancaria" />
	</class>
</hibernate-mapping>
