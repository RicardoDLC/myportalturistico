<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="SOAPServices"
                   namespace="SOAPServices.Dominio"
                   default-lazy="false">
	<class name="Cliente" table="Cliente">
		<id name="CodCliente" column="CodCliente">
			<generator class="identity" />
		</id>
		<property name="NombreCliente" column="NombreCliente" />
		<property name="ApellidoCliente" column="ApellidoCliente" />
		<property name="DNI" column="DNI" />
		<property name="CorreoCliente" column="CorreoCliente" />
	</class>
</hibernate-mapping>
